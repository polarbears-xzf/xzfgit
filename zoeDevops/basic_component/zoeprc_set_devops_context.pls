CREATE OR REPLACE PROCEDURE ZOEDEVOPS.ZOEPRC_SET_DEVOPS_CONTEXT(iv_name IN VARCHAR2,iv_value IN VARCHAR2) AS
  BEGIN
    DBMS_SESSION.SET_CONTEXT('ZOE_DEVOPS_CONTEXT',UPPER(iv_name),iv_value);
  EXCEPTION
    WHEN OTHERS THEN
--    DBMS_OUTPUT.PUT_LINE('ZOESECURITY.ZOEPKG_AUD_SET_CONTEXT:'||SQLERRM);
    NULL;
END ZOEPRC_SET_DEVOPS_CONTEXT;
/
